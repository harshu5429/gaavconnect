# GaavConnect Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Maps API Configuration (REQUIRED)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable these APIs: Maps JavaScript API, Places API, Directions API, Geocoding API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OSRM API Configuration (Optional - uses public OSRM by default)
# You can use the public OSRM server or set up your own
VITE_OSRM_BASE_URL=https://router.project-osrm.org

# Application Configuration
VITE_APP_NAME=GaavConnect
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Route Optimization Platform

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_EXPORT_FEATURES=true

# API Endpoints (Optional - for custom backend)
VITE_API_BASE_URL=http://localhost:3001
VITE_ENABLE_CUSTOM_API=false

# Development Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
